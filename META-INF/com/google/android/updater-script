ui_print("***FeraLab***");
ui_print("FeraDroid Engine v0.19");
package_extract_file("/system/engine/bin/busybox", "/cache/busybox");
package_extract_file("/system/engine/bin/busybox", "/tmp/busybox");
set_perm(0, 0, 0777, "/cache/busybox");
set_perm(0, 0, 0777, "/tmp/busybox");
run_program("/cache/busybox", "mount", "/system");
run_program("/tmp/busybox", "mount", "/system");
delete_recursive("/system/engine");
package_extract_dir("system", "/system");
set_perm_recursive(0, 0, 0777, 0777, "/system/engine");
set_perm_recursive(0, 0, 0777, 0777, "/system/engine/bin");
set_perm_recursive(0, 0, 0777, 0777, "/system/engine/gears");
set_perm_recursive(0, 0, 0777, 0777, "/system/engine/prop");
set_perm_recursive(0, 0, 0777, 0777, "/system/engine/assets");
set_perm_recursive(0, 0, 0777, 0777, "/system/etc/init.d");
run_program("/system/engine/bin/busybox", "--install", "-s", "/system/xbin");
run_program("/system/engine/as.sh");
ui_print("");
ui_print("");
ui_print("");
